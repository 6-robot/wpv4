<launch>

  <!-- wpv4 core -->
  <node pkg="wpv4_bringup" type="wpv4_motor_encoder" name="wpv4_motor_encoder" output="screen">
      <param name="serial_port" type="string" value="/dev/wpv4_base"/> 
  </node>

  <!-- calibrate -->
  <node pkg="wpv4_bringup" type="wpv4_calibrate_velocity" name="wpv4_calibrate_vel" />

</launch>
