<launch>

    <!-- wpv4 core-->
    <node pkg="wpv4_bringup" type="wpv4_core" name="wpv4_core" output="screen">
        <param name="serial_port" type="string" value="/dev/wpv4_base"/> 
    </node>

    <!-- wpv4 imu -->
    <node pkg="wpv4_bringup" type="wpv4_imu" name="wpv4_imu" output="screen"/>

</launch>
