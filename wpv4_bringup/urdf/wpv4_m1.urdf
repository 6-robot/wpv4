<?xml version="1.0"?>
<robot name="wp_v4">

  <!-- material -->
  <material name = "black">
    <color rgba = "0.01 0.01 0.01 1"/>
  </material>
  <material name = "grey">
    <color rgba = "0.2 0.2 0.2 1"/>
  </material>
  <material name = "blue">
    <color rgba = "0.2 0.2 0.9 1"/>
  </material>


<link name="base_footprint">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size="0.05 0.05 0.001" />
      </geometry>
      <material name="TransparentGreen" />
    </visual>
    <collision>
      <origin xyz="0 0 .1" rpy="0 0 0" />
      <geometry>
        <box size="0.76 0.5 .2"/>
      </geometry>
    </collision>
    <inertial>
    <mass value="10"/>
    <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
</link>

<joint name="base_joint" type="fixed">
  <origin xyz="0 0 0.09" rpy="0 0 0" />
  <parent link="base_footprint"/>
  <child link="base_link" />
</joint>

  <!-- base -->
  <link name="base_link">
  <visual>
   <geometry>
    <cylinder length="0.6" radius="0.0"/>
   </geometry>
   <origin rpy = "0 0 0" xyz = "0 0 0"/>
  </visual>
  </link>

  <!-- body -->
  <link name = "body_link">
    <visual>
      <geometry>
        <mesh filename="package://wpv4/meshes/body.dae" scale="1 1 1"/>
      </geometry>
      <origin rpy = "1.57 0 1.57" xyz = "-0.38 -0.25 0"/>
    </visual>
    <collision>
      <origin xyz="0 0 .15" rpy="0 0 0" />
      <geometry>
        <box size="0.76 0.5 0.095"/>
      </geometry>
    </collision>
    <inertial>
    <mass value="10"/>
    <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
  </link>
  <joint name = "base_to_body" type = "fixed">
    <parent link = "base_link"/>
    <child link = "body_link"/>
    <origin rpy="0 0 0" xyz="0 0 0"/> <!--pos-->
  </joint>
  <gazebo reference = "body_link">
            <turnGravityOff>false</turnGravityOff>
  </gazebo>

  <!-- wheel -->
  <link name = "lf_wheel">
    <visual>
      <geometry>
        <mesh filename="package://wpv4/meshes/wheel.dae" scale="1 1 1"/>
      </geometry>
      <origin rpy = "0 0 0" xyz = "-0.13  -0.04 -0.13"/>
      <material name = "black"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.57 0 0" />
      <geometry>
        <cylinder radius="0.13" length="0.08"/>
      </geometry>
    </collision>
    <inertial>
    <mass value="10"/>
    <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
  </link>
  <joint name = "base_to_lf_wheal" type = "continuous">
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <parent link = "base_link"/>
    <child link = "lf_wheel"/>
    <origin rpy="0 0 0" xyz="0.135 0.26 0.038"/> <!--pos to base-->
  </joint>
   <gazebo reference = "lf_wheel">
            <mu1 value = "1.0"/>
            <mu2 value = "1.0"/>
            <kp value = "10.0"/>
            <kd value = "1.0"/>
            <fdir1 value = "1 0 0"/>
            <turnGravityOff>true</turnGravityOff>
  </gazebo>

<link name = "rf_wheel">
  <visual>
    <geometry>
      <mesh filename="package://wpv4/meshes/wheel.dae" scale="1 1 1"/>
    </geometry>
    <origin rpy = "0 0 0" xyz = "-0.13  -0.04 -0.13"/>
    <material name = "black"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="1.57 0 0" />
    <geometry>
      <cylinder radius="0.13" length="0.08"/>
    </geometry>
  </collision>
  <inertial>
  <mass value="10"/>
  <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
  </inertial>
</link>
<joint name = "base_to_rf_wheal" type = "continuous">
  <axis rpy="0 0 0" xyz="0 -1 0"/>
  <parent link = "base_link"/>
  <child link = "rf_wheel"/>
  <origin rpy="0 0 3.14" xyz="0.135 -0.26 0.038"/> <!--pos-->
</joint>
 <gazebo reference = "rf_wheel">
            <mu1 value = "1.0"/>
            <mu2 value = "1.0"/>
            <kp value = "10.0"/>
            <kd value = "1.0"/>
            <fdir1 value = "1 0 0"/>
            <turnGravityOff>false</turnGravityOff>
        </gazebo>

<link name = "lb_wheel">
  <visual>
    <geometry>
      <mesh filename="package://wpv4/meshes/wheel.dae" scale="1 1 1"/>
    </geometry>
    <origin rpy = "0 0 0" xyz = "-0.13  -0.04 -0.13"/>
    <material name = "black"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="1.57 0 0" />
    <geometry>
      <cylinder radius="0.13" length="0.08"/>
    </geometry>
  </collision>
  <inertial>
  <mass value="10"/>
  <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
  </inertial>
</link>
<joint name = "base_to_lb_wheal" type = "continuous">
  <axis rpy="0 0 0" xyz="0 1 0"/>
  <parent link = "base_link"/>
  <child link = "lb_wheel"/>
  <origin rpy="0 0 0" xyz="-0.175 0.26 0.038"/> <!--pos to base-->
</joint>
 <gazebo reference = "lb_wheel">
            <mu1 value = "1.0"/>
            <mu2 value = "1.0"/>
            <kp value = "10.0"/>
            <kd value = "1.0"/>
            <fdir1 value = "1 0 0"/>
            <material>Gazebo/Black</material>
            <turnGravityOff>false</turnGravityOff>
        </gazebo>

<link name = "rb_wheel">
  <visual>
    <geometry>
      <mesh filename="package://wpv4/meshes/wheel.dae" scale="1 1 1"/>
    </geometry>
    <origin rpy = "0 0 0" xyz = "-0.13  -0.04 -0.13"/>
    <material name = "black"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="1.57 0 0" />
    <geometry>
      <cylinder radius="0.13" length="0.08"/>
    </geometry>
  </collision>
  <inertial>
  <mass value="10"/>
  <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
  </inertial>
</link>
<joint name = "base_to_rb_wheal" type = "continuous">
  <axis rpy="0 0 0" xyz="0 -1 0"/>
  <parent link = "base_link"/>
  <child link = "rb_wheel"/>
  <origin rpy="0 0 3.14" xyz="-0.175 -0.26 0.038"/> <!--pos-->
</joint>
 <gazebo reference = "rb_wheel">
            <mu1 value = "1.0"/>
            <mu2 value = "1.0"/>
            <kp value = "10.0"/>
            <kd value = "1.0"/>
            <fdir1 value = "1 0 0"/>
            <material>Gazebo/Black</material>
            <turnGravityOff>false</turnGravityOff>
        </gazebo>

<!-- Lidar -->
<link name = "laser">
  <visual>
   <geometry>
    <cylinder length="0.00" radius="0.03"/>
   </geometry>
   <origin rpy = "0 0 0" xyz = "0 0 0"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <geometry>
      <cylinder radius="0.03" length="0.03"/>
    </geometry>
  </collision>
  <inertial>
  <mass value="10"/>
  <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
  </inertial>
</link>
<joint name="laser_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0.2 0 0"  /> <!--pos-->
    <parent link="base_link" />
    <child link="laser" />
</joint>

<!-- Kinect -->
<link name = "kinect2_ir_optical_frame">
  <visual>
   <geometry>
    <!-- <box size=".25 .04 .07"/> -->
    <box size="0 0 0"/>
   </geometry>
   <origin rpy = "0 0 0" xyz = "0 0 0"/>
   <material name = "blue"/>
  </visual>
  <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <box size=".25 .04 .07"/>
      </geometry>
    </collision>
    <inertial>
    <mass value="10"/>
    <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
    </inertial>
</link>
<joint name="kinect_joint" type="fixed">
    <origin xyz="0.2 0 0.2" rpy="-1.5707963 0 -1.5707963" /> <!--pos-->
    <parent link="base_link" />
    <child link="kinect2_ir_optical_frame" />
</joint>

 <!-- ****manipulator ****-->
         <link name = "mani_base">
            <visual>
                <geometry>
                    <mesh filename="package://wpm1/meshes/wpm1_base.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "1.57 0 3.14" xyz = ".066 -0.075 0"/>
            </visual>
        </link>
        <joint name = "base_mani" type = "fixed">
            <parent link = "base_link"/>
            <child link = "mani_base"/>
            <origin xyz = "0.24 0 .16"/>
        </joint>
        <!-- mani_part_1 -->
        <link name = "mani_part_1">
            <visual>
                <geometry>
                    <mesh filename="package://wpm1/meshes/wpm1_joint1.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "0 3.14 0" xyz = "0.0492 -0.048 .0866"/>
            </visual>
        </link>
         <joint name = "mani_base_1" type = "revolute">
            <parent link = "mani_base"/>
            <child link = "mani_part_1"/>
            <axis xyz = "0 0 1"/>
            <origin xyz = "0 0 0.08"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>
        <!-- mani_part_2 -->
         <link name = "mani_part_2">
            <visual>
                <geometry>
                    <mesh filename="package://wpm1/meshes/wpm1_joint2.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "1.57 0 0" xyz = " -0.0425 0.0725 -0.0425"/>
            </visual>
        </link>
         <joint name = "mani_1_2" type = "revolute">
            <parent link = "mani_part_1"/>
            <child link = "mani_part_2"/>
            <axis xyz = "0 1 0"/>
            <origin xyz = "0 0.05 0.044"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>
        <!-- mani_part_3 -->
        <link name = "mani_part_3">
            <visual>
                <geometry>
                    <mesh filename="package://wpm1/meshes/wpm1_joint3.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "1.57 0 3.14" xyz = "0.04 -0.0886 -0.04"/>
            </visual>
        </link>
         <joint name = "mani_2_3" type = "revolute">
            <parent link = "mani_part_2"/>
            <child link = "mani_part_3"/>
            <axis xyz = "0 1 0"/>
            <origin xyz = "0 0 0.15"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>
        <!-- mani_part_4 -->
         <link name = "mani_part_4">
            <visual>
                <geometry>
                    <mesh filename="package://wpm1/meshes/wpm1_joint4.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "1.57 0 0" xyz = "-0.04 0.04 -0.001"/>
            </visual>
        </link>
         <joint name = "mani_3_4" type = "revolute">
            <parent link = "mani_part_3"/>
            <child link = "mani_part_4"/>
            <axis xyz = "0 0 1"/>
            <origin xyz = "0 -0.0486 0.057"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>
         <!-- mani_part_5 -->
        <link name = "mani_part_5">
            <visual>
               <geometry>
                    <mesh filename="package://wpm1/meshes/wpm1_joint5.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "0 0 1.57" xyz = "0.04 -0.04 -0.037"/>
            </visual>
        </link>
         <joint name = "mani_4_5" type = "revolute">
            <parent link = "mani_part_4"/>
            <child link = "mani_part_5"/>
            <axis xyz = "0 1 0"/>
            <origin xyz = "0 0 0.155"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>

        <!-- mani_part_6 palm-->
         <link name = "mani_part_6">
            <visual>
                <geometry>
                    <mesh filename="package://wpm1/meshes/wpm1_gripper.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "3.14 0 0" xyz = "-0.0525 0.033 0.1656"/>
            </visual>
        </link>
         <joint name = "mani_5_6" type = "revolute">
            <parent link = "mani_part_5"/>
            <child link = "mani_part_6"/>
            <axis xyz = "0 0 1"/>
            <origin xyz = "0 0 0.20"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>

        <!-- finger -->
         <link name = "mani_left_finger">
            <visual>
                <geometry>
                    <box size="0 0 0"/>
                </geometry>
                    <material name = "grey"/>
                <origin rpy = "1.57 0 -1.57" xyz = ".021 0.005 -0.005"/>
            </visual>
        </link>
        <joint name = "mani_palm_finger" type = "revolute">
            <parent link = "mani_part_6"/>
            <child link = "mani_left_finger"/>
            <origin xyz = "0.015 0 0.072"/>
            <axis xyz = "-1 0 0"/>
            <limit effort = "1000.0" lower="0" upper="1.0" velocity="0.5"/>
        </joint>

         <link name = "mani_right_finger">
            <visual>
                <geometry>
                    <box size="0 0 0"/>
                </geometry>
                    <material name = "grey"/>
                <origin rpy = "1.57 0 1.57" xyz = "-0.0175 -0.005 -0.005"/>
            </visual>
        </link>
        <joint name = "mani_palm_right_finger" type = "revolute">
            <parent link = "mani_part_6"/>
            <child link = "mani_right_finger"/>
            <origin xyz = "-0.015 0 0.072"/>
            <axis xyz = "1 0 0"/>
            <limit effort = "1000.0" lower="0" upper="1.0" velocity="0.5"/>
            <mimic joint = "mani_palm_finger" multiplier = "1" offset = "0"/>
        </joint>
        <!-- *****************-->

<!-- Gazebo plugin for ROS Control -->
<gazebo>
  <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
    <robotNamespace>/</robotNamespace>
  </plugin>
</gazebo>

</robot>
