</link>
<joint name = "base_to_rb_wheal" type = "continuous">
  <axis rpy="0 0 0" xyz="0 -1 0"/>
  <parent link = "base_link"/>
  <child link = "rb_wheel"/>
  <origin rpy="0 0 3.14" xyz="-0.175 -0.26 0.038"/> <!--pos-->
</joint>
 <gazebo reference = "rb_wheel">
            <mu1 value = "1.0"/>
            <mu2 value = "1.0"/>
            <kp value = "10.0"/>
            <kd value = "1.0"/>
            <fdir1 value = "1 0 0"/>
            <turnGravityOff>false</turnGravityOff>
        </gazebo>


        <!-- ****manipulator ****-->
         <link name = "mani_base">
            <visual>
                <geometry>
                    <mesh filename="package://wpm2/meshes/base_mod.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "1.57 0 1.57" xyz = "-0.067 -0.06 0"/>
            </visual>
        </link>
         <joint name = "base_mani" type = "fixed">
            <parent link = "base_link"/>
            <child link = "mani_base"/>
            <origin xyz = "0.24 0 .16"/>
        </joint>
        <!-- mani_part_1 -->
        <link name = "mani_part_1">
            <visual>
                <geometry>
                    <mesh filename="package://wpm2/meshes/joint_mod_1.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "0 0 0" xyz = "-0.046 -0.0675 0"/>
            </visual>
        </link>
         <joint name = "mani_base_1" type = "revolute">
            <parent link = "mani_base"/>
            <child link = "mani_part_1"/>
            <axis xyz = "0 0 1"/>
            <origin xyz = "0 0 0.127"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>
        <!-- mani_part_2 -->
         <link name = "mani_part_2">
            <visual>
                <geometry>
                    <mesh filename="package://wpm2/meshes/joint_mod_2.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "1.57 0 0" xyz = " -0.046 0.0675 -0.033"/>
            </visual>
        </link>
         <joint name = "mani_1_2" type = "revolute">
            <parent link = "mani_part_1"/>
            <child link = "mani_part_2"/>
            <axis xyz = "0 1 0"/>
            <origin xyz = "0 0 0.0795"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>
        <!-- mani_part_3 -->
        <link name = "mani_part_3">
            <visual>
                <geometry>
