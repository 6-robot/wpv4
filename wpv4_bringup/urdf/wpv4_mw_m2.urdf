                </geometry>
                <origin rpy = "0 0 0" xyz = "-0.046 -0.0675 0"/>
            </visual>
        </link>
         <joint name = "mani_2_3" type = "revolute">
            <parent link = "mani_part_2"/>
            <child link = "mani_part_3"/>
            <axis xyz = "0 0 1"/>
            <origin xyz = "0 0 0.1945"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>
        <!-- mani_part_4 -->
         <link name = "mani_part_4">
            <visual>
                <geometry>
                    <mesh filename="package://wpm2/meshes/joint_mod_2.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "1.57 0 0" xyz = "-0.046 0.0675 -0.033"/>
            </visual>
        </link>
         <joint name = "mani_3_4" type = "revolute">
            <parent link = "mani_part_3"/>
            <child link = "mani_part_4"/>
            <axis xyz = "0 1 0"/>
            <origin xyz = "0 0 0.0795"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>
        <!-- mani_part_5 -->
        <link name = "mani_part_5">
            <visual>
               <geometry>
                    <mesh filename="package://wpm2/meshes/joint_mod_1.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "0 0 0" xyz = "-0.046 -0.0675 0"/>
            </visual>
        </link>
         <joint name = "mani_4_5" type = "revolute">
            <parent link = "mani_part_4"/>
            <child link = "mani_part_5"/>
            <axis xyz = "0 0 1"/>
            <origin xyz = "0 0 0.1945"/>
            <limit effort = "1000.0" lower="-3.14" upper="3.14" velocity="0.5"/>
        </joint>
         <!-- mani_part_6 palm-->
         <link name = "mani_part_6">
            <visual>
                <geometry>
                    <mesh filename="package://wpm2/meshes/palm.dae" scale="1 1 1"/>
                </geometry>
                <origin rpy = "1.57 0 1.57" xyz = "-0.067 -0.062 -0.033"/>
            </visual>
        </link>
         <joint name = "mani_5_6" type = "revolute">
            <parent link = "mani_part_5"/>
