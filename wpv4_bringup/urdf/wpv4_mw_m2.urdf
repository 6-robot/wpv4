</link>
<joint name = "base_to_lb_wheal" type = "continuous">
  <axis rpy="0 0 0" xyz="0 1 0"/>
  <parent link = "base_link"/>
  <child link = "lb_wheel"/>
  <origin rpy="0 0 0" xyz="-0.175 0.26 0.038"/> <!--pos to base-->
</joint>
 <gazebo reference = "lb_wheel">
            <mu1 value = "1.0"/>
            <mu2 value = "1.0"/>
            <kp value = "10.0"/>
            <kd value = "1.0"/>
            <fdir1 value = "1 0 0"/>
            <turnGravityOff>false</turnGravityOff>
        </gazebo>

<link name = "rb_wheel">
  <visual>
    <geometry>
      <mesh filename="package://wpv4/meshes/mecanum_a.dae" scale="1 1 1"/>
      </geometry>
      <origin rpy = "1.57 0 0" xyz = "-0.10  0.07 -0.10"/>
    <material name = "black"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="1.57 0 0" />
    <geometry>
      <cylinder radius="0.13" length="0.08"/>
    </geometry>
  </collision>
  <inertial>
  <mass value="10"/>
  <inertia ixx="0.4" ixy="0.0" ixz="0.0" iyy="0.4" iyz="0.0" izz="0.2"/>
