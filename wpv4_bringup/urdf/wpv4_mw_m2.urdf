            <origin xyz = "0 -0.06 0.219"/>
            <axis xyz = "1 0 0"/>
            <limit effort = "1000.0" lower="0" upper="1.0" velocity="0.5"/>
            <mimic joint = "mani_palm_finger" multiplier = "1" offset = "0"/>
        </joint>
